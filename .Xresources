xterm*font: fixed
xterm*boldFont: fixed
xterm*allowBoldFonts: false
xterm*cjk_width: true
xterm*altSendsEscape: true

Xft.autohint: 0
Xft.dpi: 96
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

URxvt*scrollBar: false
URxvt*loginShell: true
!URxvt*font: xft:Monaco for Powerline:regular:size=8,xft:VL ゴシック:regular:size=8
!URxvt*font: xft:Monaco for Powerline:regular:size=8,xft:Droid Sans Fallback:regular:size=8
URxvt*font: xft:Monaco for Powerline:regular:size=8,xft:Droid Sans Fallback:regular:size=8,xft:Deja Vu Sans Mono:regular:size=8
!URxvt*font: xft:STIX Size Four Sym:regular:size=8
URxvt*imFont: xft:Monaco for Powerline:regular:size=8,xft:Droid Sans Fallback:regular:size=8,xft:Deja Vu Sans Mono:regular:size=8
URxvt*boldFont: xft:Monaco for Powerline:regular:size=8,xft:Droid Sans Fallback:regular:size=8,xft:Deja Vu Sans Mono:bold:size=8
URxvt*italicFont: xft:Monaco for Powerline:italic:size=8,xft:Droid Sans Fallback:italic:size=8,xft:Deja Vu Sans Mono:bold:italic:size=8
URxvt*boldItalicFont: xft:Monaco for Powerline:bold:italic:size=8,xft:Droid Sans Fallback:bold:italic:size=8,xft:Deja Vu Sans Mono:bold:italic:size=8

URxvt.perl-ext-common:      default,clipboard,url-select,keyboard-select,vtwheel
!URxvt.perl-ext-common: default,matcher,searchable-scrollback
URxvt.url-select.launcher:  firefox -new-tab
URxvt.url-select.underline: true
URxvt.keysym.M-u:           perl:url-select:select_next
URxvt.keysym.M-Escape:      perl:keyboard-select:activate
URxvt.keysym.M-s:           perl:keyboard-select:search

!Background is terminal #234, foreground is #252
!*background:            #262626
!*foreground:            #d0d0d0

URxvt*inheritPixmap: true
URxvt*tintColor: gray30
URxvt.internalBorder: 0
URxvt.jumpScroll: true
! URxvt.loginShell: true
URxvt.pointerBlank: true
URxvt.saveLines: 4000
URxvt.secondaryScroll: true
URxvt.scrollBar: false
URxvt.scrollTtyKeypress: true
URxvt.scrollWithBuffer: true
URxvt*urlLauncher: /usr/bin/firefox
